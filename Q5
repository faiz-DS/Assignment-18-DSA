def contains_duplicate(nums):
    num_set = set()
    for num in nums:
        if num in num_set:
            return True
        num_set.add(num)
    return False

# Example 1
nums1 = [1, 2, 3, 1]
print(contains_duplicate(nums1))  # Output: True

# Example 2
nums2 = [1, 2, 3, 4]
print(contains_duplicate(nums2))  # Output: False

# Example 3
nums3 = [1, 1, 1, 3, 3, 4, 3, 2, 4, 2]
print(contains_duplicate(nums3))  # Output: True
